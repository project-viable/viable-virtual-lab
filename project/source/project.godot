; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="VIABLE Virtual Lab"
run/main_scene="res://Main.tscn"
config/features=PackedStringArray("4.4")
boot_splash/image="res://Images/MainMenu-Background.png"
config/icon="res://favicon.png"

[autoload]

LabLog="*res://Scripts/LabLogSingleton.gd"
GameSettings="*res://Scripts/GameSettingsSingleton.gd"
Interaction="*res://combined_scripts/interaction_singleton.gd"
LabTime="*res://combined_scripts/lab_time_singleton.gd"
Cursor="*res://combined_scripts/cursor_singleton.gd"
Subscenes="*res://combined_scripts/subscenes_singleton.gd"
Game="*res://combined_scripts/game_singleton.gd"
DepthManager="*res://combined_scripts/depth_manager_singleton.gd"

[debug]

gdscript/warnings/untyped_declaration=2

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/size/resizable=false
window/stretch/mode="canvas_items"

[global_group]

attachment:microcentrifuge_tube=""
attachment:scoopula=""
contact_wire=""
emptyable=""
attachment:contact_wire=""
attachment:pipette=""
ghost:always_include="Always include this node in a sprite ghost (made via `Util.make_sprite_ghost`), regardless of visibility"
ghost:never_include="Never include this node in a sprite ghost (made via `Util.make_sprite_ghost`), regardless of visibility"
entry_point="A collision shape owner that must be used to enter the purview of an `ExclusiveShape2D` if it has `require_entry_point_shapes` enabled"
exclusive_area:pipette="Allow the pipette to enter this area"
attachment:gel_mold=""
zoom:scale=""

[gui]

theme/custom="res://UI/MainUITheme.tres"
theme/custom_font="res://UI/RobotoMedium16.tres"
common/drop_mouse_on_gui_input_disabled=true

